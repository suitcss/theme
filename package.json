{
  "name": "suitcss-theme",
  "description": "Example theme for SUIT CSS",
  "version": "0.1.0",
  "style": "theme.css",
  "files": [
    "index.css",
    "lib"
  ],
  "dependencies": {
    "suitcss": "^0.6.0"
  },
  "devDependencies": {
    "suitcss-preprocessor": "^0.4.0"
