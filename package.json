{
  "name": "suitcss-theme",
  "description": "Example theme for SUIT CSS",
  "version": "0.1.2",
  "license": "MIT",
  "style": "theme.css",
  "files": [
    "index.css",
    "lib"
  ],
  "dependencies": {
    "suitcss-base": "Explorys/base.git#2.0.0",
    "suitcss-components-button": "Explorys/components-button.git#5.0.0",
    "suitcss-components-data-table": "Explorys/components-data-table.git#0.1.0",
    "suitcss-components-flex-embed": "Explorys/components-flex-embed.git#2.0.4",
    "suitcss-components-form": "Explorys/components-form.git",
    "suitcss-components-grid": "Explorys/components-grid.git#3.0.3",
    "suitcss-components-text-field": "Explorys/components-text-field.git#0.1.1",
    "suitcss-utils-after": "Explorys/utils-after.git#1.0.1",
    "suitcss-utils-align": "Explorys/utils-align.git#1.0.0",
    "suitcss-utils-before": "Explorys/utils-before.git#1.0.1",
    "suitcss-utils-color": "Explorys/utils-color.git#0.1.0",
    "suitcss-utils-display": "Explorys/utils-display.git#1.0.2",
    "suitcss-utils-flex": "Explorys/utils-flex.git#1.1.1",
    "suitcss-utils-font": "Explorys/utils-font.git#0.1.1",
    "suitcss-utils-layout": "Explorys/utils-layout.git#1.0.1",
    "suitcss-utils-link": "Explorys/utils-link.git#1.0.1",
    "suitcss-utils-position": "Explorys/utils-position.git#1.0.1",
    "suitcss-utils-size": "Explorys/utils-size.git#2.0.0",
    "suitcss-utils-text": "Explorys/utils-text.git#1.0.0"
  },
  "devDependencies": {
    "kss": "^3.0.0-beta.14",
    "stylelint": "^6.6.0",
    "stylelint-config-suitcss": "^6.0.0",
    "suitcss-preprocessor": "^2.0.0"
  },
  "scripts": {
    "build": "npm run setup && npm run preprocess",
    "kss": "kss --config ./config/styleguide.json",
    "preprocess": "suitcss -c config/preprocess.json index.css build/build.css",
    "setup": "npm install",
    "styleguide": "npm run build && npm run kss"
  },
  "repository": {
    "type": "git",
    "url": "git://github.ibm.com:Explorys/theme.git"
  },
  "keywords": [
    "browser",
    "css-theme",
    "suitcss",
    "style"
  ]
}
